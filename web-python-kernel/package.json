{
  "name": "web-python-kernel",
  "version": "0.1.0",
  "description": "A web-based Python kernel for browser environments with TypeScript support",
  "main": "dist/web-python-kernel.js",
  "module": "dist/web-python-kernel.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "[ -f web-python-kernel.js ] || node --max-old-space-size=8192 ./node_modules/.bin/webpack --mode=development; cp -r src/pypi .",
    "serve": "node serve.js",
    "playground": "npm run build && npm run serve",
    "test": "node --max-old-space-size=8192 ./node_modules/.bin/karma start --single-run --browsers ChromeHeadless",
    "test:watch": "node --max-old-space-size=8192 ./node_modules/.bin/karma start --auto-watch",
    "test:chrome": "node --max-old-space-size=8192 ./node_modules/.bin/karma start --single-run --browsers Chrome",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rm -rf dist pypi web-python-kernel.js .webpack-cache",
    "rebuild": "npm run clean && node --max-old-space-size=8192 ./node_modules/.bin/webpack --mode=development && cp -r src/pypi .",
    "prepare": "npm run build"
  },
  "keywords": [
    "python",
    "kernel",
    "jupyter",
    "browser",
    "webassembly",
    "pyodide",
    "typescript"
  ],
  "author": "Web Python Kernel Team",
  "license": "MIT",
  "dependencies": {
    "comlink": "^4.4.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "chai": "^4.4.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-webpack": "^5.0.1",
    "mocha": "^10.6.1",
    "prettier": "^3.3.2",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "webpack": "^5.100.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "worker-loader": "^3.0.8"
  }
} 